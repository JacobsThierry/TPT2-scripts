:import array

:local string elems

        
elems = {array("fire;water;earth;air;nature;light;darkness;electricity;universal;neutral")}


loop:
:local int i
i = 0

loop12:
disable.era({get(elems, i)})
i = i+1
gotoif(loop12, i < {length(elems)})

gotoif(loop, isTowerTesting() & (0.0 < health(true)))
